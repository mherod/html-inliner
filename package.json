{
  "name": "@mherod/html-inliner",
  "version": "1.2.0",
  "description": "",
  "source": "src/index.ts",
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm-run-all build:*",
    "build:lib": "parcel build --target lib",
    "build:cli": "parcel build --target cli",
    "build:types": "parcel build --target types",
    "install-global": "npm run build && npm install --force --global . && find $NVM_BIN -exec chmod +x {} \\;",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@parcel/validator-typescript": "^2.7.0",
    "@prettier/plugin-xml": "^2.2.0",
    "@types/clean-css": "^4.2.6",
    "@types/csso": "^5.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.186",
    "@types/mime-types": "^2.1.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.8.3",
    "@types/postcss-js": "^4.0.0",
    "@types/prettier": "^2.7.1",
    "@types/svgo": "^2.6.4",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "colorette": "^2.0.19",
    "cross-fetch": "^3.1.5",
    "cssnano": "^5.1.13",
    "jsdom": "^20.0.1",
    "lodash": "^4.17.21",
    "lru-cache": "^7.14.0",
    "mime-types": "^2.1.35",
    "minimist": "^1.2.6",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "svgo": "^2.8.0"
  },
  "targets": {
    "main": {},
    "lib": {
      "source": "src/index.ts",
      "distDir": "dist",
      "context": "node",
      "optimize": true,
      "includeNodeModules": false,
      "isLibrary": true
    },
    "cli": {
      "source": "src/cli.ts",
      "distDir": "dist",
      "context": "node",
      "optimize": true,
      "isLibrary": false,
      "sourceMap": false,
      "includeNodeModules": {
        "jsdom": false,
        "prettier": false,
        "svgo": false,
        "postcss": false,
        "cssnano": false
      }
    }
  },
  "engines": {
    "node": ">= 14"
  },
  "@parcel/transformer-js": {
    "inlineFS": false,
    "inlineEnvironment": false
  }
}
