{
  "name": "@mherod/html-inliner",
  "version": "1.1.2",
  "description": "",
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:main && npm run build:cli",
    "build:main": "parcel build --target main",
    "build:cli": "parcel build --target cli",
    "install-global": "npm run build && npm install --force --global . && find $NVM_BIN -exec chmod +x {} \\;",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@parcel/validator-typescript": "^2.7.0",
    "@types/clean-css": "^4.2.6",
    "@types/jsdom": "^20.0.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.8.3",
    "@types/prettier": "^2.7.1",
    "parcel": "^2.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "clean-css": "^5.3.1",
    "colorette": "^2.0.19",
    "cross-fetch": "^3.1.5",
    "jsdom": "^20.0.1",
    "lru-cache": "^7.14.0",
    "mime-types": "^2.1.35",
    "prettier": "^2.7.1"
  },
  "targets": {
    "main": {
      "source": "index.ts",
      "context": "node",
      "optimize": true,
      "includeNodeModules": false
    },
    "cli": {
      "source": "cli.js",
      "context": "node",
      "optimize": true,
      "includeNodeModules": [
        "lru-cache",
        "cross-fetch",
        "colorette"
      ]
    }
  },
  "engines": {
    "node": ">= 14"
  },
  "@parcel/transformer-js": {
    "inlineFS": false,
    "inlineEnvironment": false
  }
}
